    "@babel/eslint-parser": "^7.22.15",
    "@babel/preset-env": "7.10.4",
    "babel-loader": "8.1.0",
    "concurrently": "^8.2.1",
    "css-loader": "5.2.7",
    "eslint": "^8.40.0",
    "eslint-config-alloy": "^5.1.2",
    "eslint-plugin-prettier": "5.0.0",
    "mini-css-extract-plugin": "1.6.2",
    "nodemon": "3.0.1",
    "prettier": "3.0.3",
    "sass": "1.67.0",
    "sass-loader": "10.4.1",
